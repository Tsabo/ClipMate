<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="DevExpress Online" value="https://nuget.devexpress.com/api" />
  </packageSources>
  
  <!-- Package Source Mapping for Central Package Management -->
  <packageSourceMapping>
    <!-- DevExpress packages -->
    <packageSource key="DevExpress Online">
      <package pattern="DevExpress.*" />
    </packageSource>
    
    <!-- All other packages from nuget.org -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
