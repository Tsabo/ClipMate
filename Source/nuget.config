<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Package Source Mapping for Central Package Management -->
  <packageSourceMapping>
    <!-- DevExpress packages -->
    <packageSource key="DevExpress Online">
      <package pattern="DevExpress.*" />
    </packageSource>
    
    <!-- All other packages from nuget.org -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
